{
  "name": "sage",
  "version": "9.0.9",
  "author": "Roots <team@roots.io>",
  "homepage": "https://roots.io/sage/",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/roots/sage.git"
  },
  "bugs": {
    "url": "https://github.com/roots/sage/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/MIT"
    }
  ],
  "browserslist": [
    "last 2 versions",
    "android 4",
    "opera 12"
  ],
  "scripts": {
    "build": "webpack --progress --config resources/assets/build/webpack.config.js",
    "build:production": "webpack --mode=\"production\" --progress --config resources/assets/build/webpack.config.js",
    "build:profile": "webpack --progress --profile --json --config resources/assets/build/webpack.config.js",
    "//start": "webpack --watch --config resources/assets/build/webpack.config.js",
    "start": "webpack serve --watch --config resources/assets/build/webpack.config.js",
    "rmdist": "rimraf dist",
    "lint": "npm run -s lint:scripts && npm run -s lint:styles",
    "lint:scripts": "eslint resources/assets/scripts resources/assets/build",
    "lint:styles": "stylelint \"resources/assets/styles/**/*.{css,sass,scss,sss,less}\"",
    "test": "npm run -s lint",
    "setup": "node setup.js",
    "//postinstall": "patch-package"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.13",
    "@wordpress/is-shallow-equal": "^3.0.1",
    "autoprefixer": "^10.1.0",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.26.13",
    "browser-sync-webpack-plugin": "^2.3.0",
    "bufferhelper": "^0.2.1",
    "cache-loader": "^4.1.0",
    "chokidar": "^3.5.0",
    "clean-webpack-plugin": "^3.0.0",
    "concat-stream": "^2.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "get-port": "^5.1.1",
    "mini-css-extract-plugin": "^1.3.3",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.2",
    "postcss-loader": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "react": "^17.0.1",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.11.1",
    "webpack-assets-manifest": "^5.0.0",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.1",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^5.7.3",
    "yargs": "^16.2.0"
  },
  "dependencies": {
    "@ryangjchandler/spruce": "^2.4.0",
    "@wordpress/blocks": "^7.0.2",
    "alpinejs": "^2.8.0",
    "dom-focus-lock": "^1.0.4",
    "sockjs-client": "^1.5.0",
    "tailwindcss": "^2.0.2"
  }
